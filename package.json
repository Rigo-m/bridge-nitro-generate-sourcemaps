{
  "name": "bridge-testt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxi dev --port 1080",
    "storybook": "nuxt storybook",
    "storybook:build": "nuxt storybook build",
    "build": "NODE_OPTIONS='--max-old-space-size=8192' nuxi generate",
    "start": "node functions/nitro/server/index.mjs",
    "generate": "nuxi generate",
    "lint:js": "eslint --fix --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "vetur-doc": "vue-int --output 'vetur' --input 'components' --recursive",
    "prepare": "husky install",
    "setup": "nuxi prepare",
    "move:builder": "mv .output/server functions/server"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "core-js": "^3.15.1",
    "nuxt-edge": "latest"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@nuxt/bridge": "npm:@nuxt/bridge-edge@3.0.0-27328916.daec432",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "prettier": "^2.3.2",
    "sass": "^1.49.7",
    "sass-loader": "^10"
  }
}
